        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": "●"
        },
        "docwalker_kind_guess": "list_item"
      },
      {
        "index": 1168,
        "text": "ไพลิน: ฉันอบเค้กก้อนนี้มาให้คุณ",
        "namedStyleType": "NORMAL_TEXT",
        "bullet": null,
        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": null
        },
        "docwalker_kind_guess": "paragraph"
      },
      {
