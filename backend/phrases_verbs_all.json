        "docwalker_kind_guess": "list_item"
      },
      {
        "index": 2229,
        "text": "",
        "namedStyleType": "NORMAL_TEXT",
        "bullet": null,
        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": null
        },
        "docwalker_kind_guess": "paragraph"
      },
      {
        "index": 2230,
        "text": "\tดูการใช้เพิ่มเติมของ ‘Wow’",
        "namedStyleType": "NORMAL_TEXT",
        "bullet": null,
        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": null
        },
        "docwalker_kind_guess": "paragraph"
      },
      {
        "index": 2231,
