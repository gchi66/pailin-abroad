        "index": 3839,
        "text": "TAGS",
        "namedStyleType": "NORMAL_TEXT",
        "bullet": null,
        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": null
        },
        "docwalker_kind_guess": "paragraph"
      },
