        },
        "docwalker_kind_guess": "list_item"
      },
      {
        "index": 2828,
        "text": "การพยายามตามหาหรือค้นหาบางสิ่งบางอย่าง",
        "namedStyleType": "NORMAL_TEXT",
        "bullet": null,
        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": null
        },
        "docwalker_kind_guess": "paragraph"
