      },
      {
        "index": 2826,
        "text": "LOOK FOR มองหา",
        "namedStyleType": "NORMAL_TEXT",
        "bullet": null,
        "resolvedListGlyph": {
          "glyphType": null,
          "glyphSymbol": null
        },
        "docwalker_kind_guess": "paragraph"
      },
      {
        "index": 2827,
        "text": "Look for มองหา [audio:phrases_verbs_look_for_1]",
        "namedStyleType": "NORMAL_TEXT",
